<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/profile_graph"
    app:startDestination="@id/userProfileFragment">

    <fragment
        android:id="@+id/userProfileFragment"
        android:name="can.lucky.of.profile.ui.fragments.UserProfileFragment"
        tools:layout="@layout/fragment_user_profile"
        android:label="UserProfileFragment" >
        <action
            android:id="@+id/action_userProfileFragment_to_editUserProfileFragment"
            app:destination="@id/editUserProfileFragment" />
    </fragment>
    <fragment
        android:id="@+id/editUserProfileFragment"
        android:name="can.lucky.of.profile.ui.fragments.EditUserProfileFragment"
        tools:layout="@layout/fragment_edit_user_profile"
        android:label="EditUserProfileFragment" />
</navigation>